class Solution {
public:
    void sortColors(vector<int>& nums) {
        int n = nums.size();
        map<int, int> mp;
        for(int i : nums) {
            mp[i]++;
        }
        
        int index = 0;
        for(auto it = mp.begin(); it != mp.end(); ++it) {
            while(it->second != 0) {
                nums[index] = it->first;
                index++;
                it->second--;
            }
        }
    }
};
