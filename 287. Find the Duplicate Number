class Solution {
public:
    int findDuplicate(vector<int>& nums) {
        int n=nums.size();
        unordered_map<int,int>mp;
        int count=0;
        for(int i=0;i<n;i++)
        {
            mp[nums[i]]++;
        }
        for(int num:nums)
        {
            if(mp[num]>=2)
            count++;
        }
        return count;
    }
};
