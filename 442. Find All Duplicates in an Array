class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
       map<int,int>mp;
        
        for(int i:nums)
        {
            mp[i]++;
        }
        nums.clear();
        for(auto it=mp.begin();it!=mp.end();it++)
        {
           if(it->second==2)
           {
               nums.push_back(it->first);
           }
        }
        return nums;
    }
};
