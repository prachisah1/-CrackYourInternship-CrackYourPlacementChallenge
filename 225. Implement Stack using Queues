class MyStack {
public:
    std::queue<int> q1;
    std::queue<int> q2;

    MyStack() {

    }
    
    void push(int x) {
        q2.push(x);
        while (!q1.empty()) {
            q2.push(q1.front());
            q1.pop();
        }
        swap(q1, q2);
    }
    
    int pop() {
        int re = q1.front();
        q1.pop();
        return re;  
    }
    
    int top() {
        return q1.front();
    }
    
    bool empty() {
        return q1.empty();
    }
};
