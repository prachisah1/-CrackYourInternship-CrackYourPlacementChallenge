class Solution {
public:
    bool isValid(string s) {
      stack<char>stack;
        for(char c:s)
        {
             if(c=='('||c=='{'||c=='[')        
 {
 stack.push(c);
 }         
        else
            {
 if(stack.empty()) return false;
            char a=stack.top();
          stack.pop();  
            
     if(c==')'&&a!='('||c=='}'&&a!='{'||c==']'&&a!='[')
                {
                return false; 
                }
            }
    }
        return stack.empty();
    }
};
