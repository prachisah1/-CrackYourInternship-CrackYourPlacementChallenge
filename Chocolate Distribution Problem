class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int n=prices.size();
        int sell=0;
        int buy=INT_MAX;
        
        for(int i=0;i<n;i++)
        {
          buy=min(prices[i],buy);
          int profit=prices[i]-buy;
          sell=max(profit,sell);
        }
        return sell;
    }
};
